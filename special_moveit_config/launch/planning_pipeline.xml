<launch>

  <!-- This file makes it easy to include different planning pipelines; 
       It is assumed that all planning pipelines are named XXX_planning_pipeline.xml  -->  

  <arg name="robot" default="cob3-3"/>
  <arg name="pipeline" default="ompl" />

  <include file="$(find special_moveit_config)/launch/$(arg pipeline)_planning_pipeline.xml">
      <arg name="robot" value="$(arg robot)"/>
  </include>

</launch>
